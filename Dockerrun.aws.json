{
  "AWSEBDockerrunVersion": "3",
  "Image": {
    "Name": "amazon/amazon-linux:latest"
  },
  "PortMappings": [
    {
      "HostPort": 80,
      "ContainerPort": 80
    },
    {
      "HostPort": 8080,
      "ContainerPort": 8080
    }
  ],
  "Volumes": [
    {
      "Host": {
        "SourcePath": "/var/app/current/docker-compose.yml"
      },
      "Container": {
        "SourcePath": "/docker-compose.yml"
      }
    }
  ],
  "MountPoints": [
    {
      "SourceVolume": "awseb-logs-task-ui",
      "ContainerPath": "/var/log/nginx"
    }
  ],
  "Logging": "/var/log/nginx"
}

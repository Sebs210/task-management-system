{
  "AWSEBDockerrunVersion": "3",
  "containerDefinitions": [
    {
      "name": "task-api",
      "image": "task-api",
      "essential": true,
      "memory": 256,
      "portMappings": [
        {
          "hostPort": 8080,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_DATASOURCE_URL",
          "value": "jdbc:mysql://task-management-db.cknw6is8qybw.us-east-1.rds.amazonaws.com:3306/task_management"
        },
        {
          "name": "SPRING_DATASOURCE_USERNAME",
          "value": "admin"
        },
        {
          "name": "SPRING_DATASOURCE_PASSWORD",
          "value": "MySecurePass123"
        }
      ]
    },
    {
      "name": "task-ui",
      "image": "task-ui",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ]
    }
  ]
}
